        <activity android:name=".View.OverzichtVrijwilliger" />
        <activity android:name=".Model.Vrijwilliger" />
        <activity android:name=".View.SecondCursist" />
        <activity android:name=".Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.OverzichtCursist"
            tools:ignore="WrongManifestParent" />
